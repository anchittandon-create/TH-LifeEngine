# Production Environment Variables
# Copy these to your hosting platform's environment variables section

GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE
OPENAI_API_KEY=sk-YOUR_OPENAI_KEY
FIREBASE_PROJECT_ID=your_production_project_id
FIREBASE_CLIENT_EMAIL=your_production_client_email
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_production_private_key\n-----END PRIVATE KEY-----\n"
LIFEENGINE_ENABLED=true
LIFEENGINE_ROLLOUT_PERCENT=100
LIFEENGINE_COACH_MINI_ENABLED=false

# Update this with your actual production domain
NEXT_PUBLIC_BASE_URL=https://your-domain.com/application/TH_LifeEngine
NEXT_PUBLIC_LIFEENGINE_GPT_URL=https://chatgpt.com/g/YOUR_GPT_ID_HERE
NEXT_PUBLIC_LIFEENGINE_GPT_ID=g-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
LIFEENGINE_CUSTOM_GPT_ID=g-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Optional: Disable Next.js telemetry in production
NEXT_TELEMETRY_DISABLED=1
